<!--component/pickerYMDHM/pickerYMDHM.wxml-->


<view>
  <picker disabled="{{disabled}}" mode="multiSelector" bindchange="pickerChange" bindcolumnchange="pickerColumnChange" bindcancel ="pickerCancel" value="{{pickerIndex}}" range="{{pickerArray}}" range-key="{{'name'}}">
    <view style='font-size:28rpx;background-color: #f0f0ff;height: 40rpx;line-height: 40rpx;padding:20rpx 30rpx;width:150px;text-align:center'>
      {{dateString}}
    </view>
  </picker>
</view>
